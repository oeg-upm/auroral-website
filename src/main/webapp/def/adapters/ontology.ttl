#################################################################
#    Object Properties
#################################################################

###  https://auroral.iot.linkeddata.es/def/core#hasReadability
<https://auroral.iot.linkeddata.es/def/core#hasReadability> rdf:type owl:ObjectProperty ;
                                                            rdfs:domain <https://saref.etsi.org/core#Measurement> ;
                                                            rdfs:range <https://auroral.iot.linkeddata.es/def/core#Readability> ;
                                                            rdfs:comment "A relationship between the measurement of its readability status" ;
                                                            rdfs:label "has readability" .


###  https://auroral.iot.linkeddata.es/def/core#hasTemporalUnit
<https://auroral.iot.linkeddata.es/def/core#hasTemporalUnit> rdf:type owl:ObjectProperty ;
                                                             rdfs:domain <https://saref.etsi.org/core#Measurement> ;
                                                             rdfs:range <https://www.w3.org/2006/time#TemporalUnit> ;
                                                             rdfs:comment "The granularity of a detected measurement" ;
                                                             rdfs:label "has temporal unit" .


###  https://saref.etsi.org/core#controlsProperty
<https://saref.etsi.org/core#controlsProperty> rdf:type owl:ObjectProperty ;
                                               rdfs:subPropertyOf owl:topObjectProperty ;
                                               rdfs:domain <https://saref.etsi.org/core#Device> ;
                                               rdfs:range <https://saref.etsi.org/core#Property> ;
                                               rdfs:comment "A relationship specifying the property that can be controlled by a certain device" ;
                                               rdfs:label "controls property" .


###  https://saref.etsi.org/core#isMeasuredIn
<https://saref.etsi.org/core#isMeasuredIn> rdf:type owl:ObjectProperty ;
                                           rdfs:domain <https://saref.etsi.org/core#Measurement> ;
                                           rdfs:range <https://saref.etsi.org/core#UnitOfMeasure> ;
                                           rdfs:comment "A relationship between the measurement and its unit of measure" ;
                                           rdfs:label "is measured in" .


###  https://saref.etsi.org/core#makesMeasurement
<https://saref.etsi.org/core#makesMeasurement> rdf:type owl:ObjectProperty ;
                                               rdfs:domain <https://saref.etsi.org/core#Device> ;
                                               rdfs:range <https://saref.etsi.org/core#Measurement> ;
                                               rdfs:comment "A relation between a device and the measurements it makes. Such measurement will link together the value of the measurement, its unit of measure and the property to which it relates." ;
                                               rdfs:label "makes measurement" .


###  https://saref.etsi.org/core#measuresProperty
<https://saref.etsi.org/core#measuresProperty> rdf:type owl:ObjectProperty ;
                                               rdfs:domain <https://saref.etsi.org/core#Device> ;
                                               rdfs:range <https://saref.etsi.org/core#Property> ;
                                               rdfs:comment "A relationship specifying the property that can be measured by a certain device" ;
                                               rdfs:label "measures property" .


###  https://saref.etsi.org/core#relatesToMeasurement
<https://saref.etsi.org/core#relatesToMeasurement> rdf:type owl:ObjectProperty ;
                                                   rdfs:domain <https://saref.etsi.org/core#Property> ;
                                                   rdfs:range <https://saref.etsi.org/core#Measurement> ;
                                                   rdfs:comment "A relationship between a property and the measurements it relates to" ;
                                                   rdfs:label "relates to measurement" .


###  https://saref.etsi.org/core#relatesToProperty
<https://saref.etsi.org/core#relatesToProperty> rdf:type owl:ObjectProperty ;
                                                rdfs:comment "A relationship between a measurement and the property it relates to" ;
                                                rdfs:label "relates to property" .


#################################################################
#    Data properties
#################################################################

###  https://auroral.iot.linkeddata.es/def/adapters#dateLastStatusUpdate
:dateLastStatusUpdate rdf:type owl:DatatypeProperty ;
                      rdfs:domain :StatusProperty ;
                      rdfs:range xsd:dateTime ;
                      rdfs:comment "The last date of updation the status of the device" ;
                      rdfs:label "date last status update" .


###  https://auroral.iot.linkeddata.es/def/adapters#hasHeight
:hasHeight rdf:type owl:DatatypeProperty ;
           rdfs:domain :SoilMoisture ;
           rdfs:range xsd:float ;
           rdfs:comment "The height level of the soil moisture" ;
           rdfs:label "has height" .


###  https://auroral.iot.linkeddata.es/def/adapters#hasVolume
:hasVolume rdf:type owl:DatatypeProperty ;
           rdfs:domain :SoilMoisture ;
           rdfs:range xsd:float ;
           rdfs:comment "The volume of the soil moisture" ;
           rdfs:label "has volume" .


###  https://auroral.iot.linkeddata.es/def/adapters#title
:title rdf:type owl:DatatypeProperty ;
       rdfs:domain :StatusProperty ;
       rdfs:range xsd:string ;
       rdfs:comment "The title of the status property" ;
       rdfs:label "title" .


###  https://auroral.iot.linkeddata.es/def/core#hasId
<https://auroral.iot.linkeddata.es/def/core#hasId> rdf:type owl:DatatypeProperty ;
                                                   rdfs:range xsd:string ;
                                                   rdfs:comment "The identifier of a given entity" ;
                                                   rdfs:label "has id" .


###  https://auroral.iot.linkeddata.es/def/core#serialNumber
<https://auroral.iot.linkeddata.es/def/core#serialNumber> rdf:type owl:DatatypeProperty ;
                                                          rdfs:domain <https://saref.etsi.org/core#Device> ;
                                                          rdfs:range xsd:string ;
                                                          rdfs:comment "The serial number of a device" ;
                                                          rdfs:label "serial number" .


###  https://auroral.iot.linkeddata.es/def/core#value
<https://auroral.iot.linkeddata.es/def/core#value> rdf:type owl:DatatypeProperty ;
                                                   rdfs:domain <https://auroral.iot.linkeddata.es/def/core#Readability> ;
                                                   rdfs:range xsd:integer ;
                                                   rdfs:comment "The readability value of the detected measurement" ;
                                                   rdfs:label "value" .


###  https://auroral.iot.linkeddata.es/def/core#versionNumber
<https://auroral.iot.linkeddata.es/def/core#versionNumber> rdf:type owl:DatatypeProperty ;
                                                           rdfs:domain <https://saref.etsi.org/core#Device> ;
                                                           rdfs:range xsd:string ;
                                                           rdfs:comment "The version number of the employed device" ;
                                                           rdfs:label "version number" .


###  https://saref.etsi.org/core#hasTimeStamp
<https://saref.etsi.org/core#hasTimeStamp> rdf:type owl:DatatypeProperty ;
                                           rdfs:range xsd:dateTime ;
                                           rdfs:comment "The timestapm of a given entity" ;
                                           rdfs:label "has time stamp" .


###  https://saref.etsi.org/core#hasValue
<https://saref.etsi.org/core#hasValue> rdf:type owl:DatatypeProperty ;
                                       rdfs:range xsd:float ;
                                       rdfs:comment "The value of the detected measurement" ;
                                       rdfs:label "has value" .


#################################################################
#    Classes
#################################################################

###  https://auroral.iot.linkeddata.es/def/adapters#Accelerometer
:Accelerometer rdf:type owl:Class ;
               rdfs:subClassOf <https://auroral.iot.linkeddata.es/def/core#Sensor> ;
               rdfs:comment "An instrument for measuring acceleration or for detecting and measuring vibrations" ;
               rdfs:label "Accelerometer" .


###  https://auroral.iot.linkeddata.es/def/adapters#AccumulatedPercipitation
:AccumulatedPercipitation rdf:type owl:Class ;
                          rdfs:subClassOf <https://saref.etsi.org/core#Property> ;
                          rdfs:comment "Accumulated precipitation is the sum of precipitation over a certain period of time in a certain place." ;
                          rdfs:label "Accumulated Percipitation" .


###  https://auroral.iot.linkeddata.es/def/adapters#ActReason
:ActReason rdf:type owl:Class ;
           rdfs:subClassOf :StatusProperty ;
           rdfs:comment "Refers to the act reason of the devices, that change the status of the device from active to passive  or vice versa" ;
           rdfs:label "Act reason" .


###  https://auroral.iot.linkeddata.es/def/adapters#AirConditionningBasic
:AirConditionningBasic rdf:type owl:Class ;
                       rdfs:subClassOf :CoolingSensor ,
                                       :HeatingSensor ;
                       rdfs:comment "Air conditioning is the controlling of air temperature, its humidity, its purity, noise- free and its flow rate at a certain place to produce a comfort zone." ;
                       rdfs:label "Air Conditionning Basic" .


###  https://auroral.iot.linkeddata.es/def/adapters#AirQuality
:AirQuality rdf:type owl:Class ;
            rdfs:subClassOf :AmbientProperty ;
            rdfs:comment "Air quality refers to the degree to which the air is suitable or clean enough for humans or the environment." ;
            rdfs:label "Air Quality" .


###  https://auroral.iot.linkeddata.es/def/adapters#AirQualitySensor
:AirQualitySensor rdf:type owl:Class ;
                  rdfs:subClassOf <https://auroral.iot.linkeddata.es/def/core#Sensor> ;
                  rdfs:comment "An air quality sensor is a device used to detect the concentration of pollutants in the air. It can usually measure two or more air quality indicators such as temperature, humidity, carbon dioxide, carbon monoxide, ozone, formaldehyde, benzene, PM2.5, PM10, etc." ;
                  rdfs:label "Air Quality Sensor" .


###  https://auroral.iot.linkeddata.es/def/adapters#Alert
:Alert rdf:type owl:Class ;
       rdfs:subClassOf :StatusProperty ;
       rdfs:comment "A mechanism that gives an audible, visual or other kind of alarm signal to alert someone to a problem or condition that requires urgent attention." ;
       rdfs:label "Alert" .


###  https://auroral.iot.linkeddata.es/def/adapters#AmbientProperty
:AmbientProperty rdf:type owl:Class ;
                 rdfs:subClassOf <https://saref.etsi.org/core#Property> ;
                 rdfs:comment "This concepts regroups the ambient properties that are detcetd by the deployed devices. These properties are the ambient temperature, the ambient humidity, the ambient flood, the ambient wind and the ambient solar radiation properties." ;
                 rdfs:label "Ambient Property" .


###  https://auroral.iot.linkeddata.es/def/adapters#AmbientSolarRadiation
:AmbientSolarRadiation rdf:type owl:Class ;
                       rdfs:subClassOf :AmbientProperty ;
                       rdfs:comment "Often called the solar resource or just sunlight, is a general term for the electromagnetic radiation emitted by the sun." ;
                       rdfs:label "Ambient Solar Radiation" .


###  https://auroral.iot.linkeddata.es/def/adapters#Ammonia
:Ammonia rdf:type owl:Class ;
         rdfs:subClassOf :AirQuality ;
         rdfs:comment "Ammonia is an inorganic chemical compound of nitrogen and hydrogen with the formula NH3. A stable binary hydride and the simplest pnictogen hydride, ammonia is a colourless gas with a distinct pungent smell." ;
         rdfs:label "Ammonia" .


###  https://auroral.iot.linkeddata.es/def/adapters#BoilerBiomass
:BoilerBiomass rdf:type owl:Class ;
               rdfs:subClassOf :BoilerSensor ;
               rdfs:comment "biomass boilers are boilers that run on biomass fuels." ;
               rdfs:label "Boiler Biomass" .


###  https://auroral.iot.linkeddata.es/def/adapters#BoilerNgas
:BoilerNgas rdf:type owl:Class ;
            rdfs:subClassOf :BoilerSensor ;
            rdfs:comment "A sensor boilers that is run on natural gas" ;
            rdfs:label "Boiler Ngas" .


###  https://auroral.iot.linkeddata.es/def/adapters#BoilerOil
:BoilerOil rdf:type owl:Class ;
           rdfs:subClassOf :BoilerSensor ;
           rdfs:comment "An oil boiler works in a very similar way to a gas boiler. The fuel, in this case oil." ;
           rdfs:label "Boiler Oil" .


###  https://auroral.iot.linkeddata.es/def/adapters#BoilerSensor
:BoilerSensor rdf:type owl:Class ;
              rdfs:subClassOf :HeatingSensor ;
              rdfs:comment "A boiler is a device which burns gas, oil, electricity, or coal in order to provide hot water, especially for the central heating in a building" ;
              rdfs:label "Boiler Sensor" .


###  https://auroral.iot.linkeddata.es/def/adapters#CarbonDioxide
:CarbonDioxide rdf:type owl:Class ;
               rdfs:subClassOf :AirQuality ;
               rdfs:comment "Carbon dioxide is a chemical compound with the chemical formula CO2. It is made up of molecules that each have one carbon atom covalently double bonded to two oxygen atoms." ;
               rdfs:label "Carbon Dioxide" .


###  https://auroral.iot.linkeddata.es/def/adapters#CarbonMonoxide
:CarbonMonoxide rdf:type owl:Class ;
                rdfs:subClassOf :AirQuality ;
                rdfs:comment "Carbon monoxide (CO) is a colorless, practically odorless, and tasteless gas or liquid. It results from incomplete oxidation of carbon in combustion. Burns with a violet flame." ;
                rdfs:label "Carbon Monoxide" .


###  https://auroral.iot.linkeddata.es/def/adapters#CloudCover
:CloudCover rdf:type owl:Class ;
            rdfs:subClassOf :AmbientProperty ;
            rdfs:comment "Cloud cover (also known as cloudiness, cloudage, or cloud amount) refers to the fraction of the sky obscured by clouds on average when observed from a particular location." ;
            rdfs:label "Cloud Cover" .


###  https://auroral.iot.linkeddata.es/def/adapters#CoarseParticulateMatter
:CoarseParticulateMatter rdf:type owl:Class ;
                         rdfs:subClassOf :AirQuality ;
                         rdfs:comment "Coarse particles (also known as PM10-2.5): particles with diameters generally larger than 2.5 µm and smaller than, or equal to, 10 µm in diameter." ;
                         rdfs:label "Coarse Particulate Matter" .


###  https://auroral.iot.linkeddata.es/def/adapters#CoolingSensor
:CoolingSensor rdf:type owl:Class ;
               rdfs:subClassOf <https://auroral.iot.linkeddata.es/def/core#Sensor> ;
               rdfs:comment "The Cooling Sensor is used to measure the temperature of the engine coolant." ;
               rdfs:label "Cooling Sensor" .


###  https://auroral.iot.linkeddata.es/def/adapters#EnergyMonitor
:EnergyMonitor rdf:type owl:Class ;
               rdfs:subClassOf <https://auroral.iot.linkeddata.es/def/core#Sensor> ;
