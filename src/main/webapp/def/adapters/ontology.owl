    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/elements/1.1/creator -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator"/>
    


    <!-- http://purl.org/dc/elements/1.1/description -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/description"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


    <!-- http://purl.org/dc/terms/license -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
    


    <!-- http://www.w3.org/ns/sosa/description -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/ns/sosa/description"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://auroral.iot.linkeddata.es/def/core#hasReadability -->


    <owl:ObjectProperty rdf:about="https://auroral.iot.linkeddata.es/def/core#hasReadability">
        <rdfs:domain rdf:resource="https://saref.etsi.org/core#Measurement"/>
        <rdfs:range rdf:resource="https://auroral.iot.linkeddata.es/def/core#Readability"/>
        <rdfs:comment>A relationship between the measurement of its readability status</rdfs:comment>
        <rdfs:label>has readability</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://auroral.iot.linkeddata.es/def/core#hasTemporalUnit -->


    <owl:ObjectProperty rdf:about="https://auroral.iot.linkeddata.es/def/core#hasTemporalUnit">
        <rdfs:domain rdf:resource="https://saref.etsi.org/core#Measurement"/>
        <rdfs:range rdf:resource="https://www.w3.org/2006/time#TemporalUnit"/>
        <rdfs:comment>The granularity of a detected measurement</rdfs:comment>
        <rdfs:label>has temporal unit</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/core#controlsProperty -->


    <owl:ObjectProperty rdf:about="https://saref.etsi.org/core#controlsProperty">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://saref.etsi.org/core#Device"/>
        <rdfs:range rdf:resource="https://saref.etsi.org/core#Property"/>
        <rdfs:comment>A relationship specifying the property that can be controlled by a certain device</rdfs:comment>
        <rdfs:label>controls property</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/core#isMeasuredIn -->


    <owl:ObjectProperty rdf:about="https://saref.etsi.org/core#isMeasuredIn">
        <rdfs:domain rdf:resource="https://saref.etsi.org/core#Measurement"/>
        <rdfs:range rdf:resource="https://saref.etsi.org/core#UnitOfMeasure"/>
        <rdfs:comment>A relationship between the measurement and its unit of measure</rdfs:comment>
        <rdfs:label>is measured in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/core#makesMeasurement -->


    <owl:ObjectProperty rdf:about="https://saref.etsi.org/core#makesMeasurement">
        <rdfs:domain rdf:resource="https://saref.etsi.org/core#Device"/>
        <rdfs:range rdf:resource="https://saref.etsi.org/core#Measurement"/>
        <rdfs:comment>A relation between a device and the measurements it makes. Such measurement will link together the value of the measurement, its unit of measure and the property to which it relates.</rdfs:comment>
        <rdfs:label>makes measurement</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/core#measuresProperty -->


    <owl:ObjectProperty rdf:about="https://saref.etsi.org/core#measuresProperty">
        <rdfs:domain rdf:resource="https://saref.etsi.org/core#Device"/>
        <rdfs:range rdf:resource="https://saref.etsi.org/core#Property"/>
        <rdfs:comment>A relationship specifying the property that can be measured by a certain device</rdfs:comment>
        <rdfs:label>measures property</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/core#relatesToMeasurement -->


    <owl:ObjectProperty rdf:about="https://saref.etsi.org/core#relatesToMeasurement">
        <rdfs:domain rdf:resource="https://saref.etsi.org/core#Property"/>
        <rdfs:range rdf:resource="https://saref.etsi.org/core#Measurement"/>
        <rdfs:comment>A relationship between a property and the measurements it relates to</rdfs:comment>
        <rdfs:label>relates to measurement</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://saref.etsi.org/core#relatesToProperty -->


    <owl:ObjectProperty rdf:about="https://saref.etsi.org/core#relatesToProperty">
        <rdfs:comment>A relationship between a measurement and the property it relates to</rdfs:comment>
        <rdfs:label>relates to property</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#dateLastStatusUpdate -->


    <owl:DatatypeProperty rdf:about="https://auroral.iot.linkeddata.es/def/adapters#dateLastStatusUpdate">
        <rdfs:domain rdf:resource="https://auroral.iot.linkeddata.es/def/adapters#StatusProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>The last date of updation the status of the device</rdfs:comment>
        <rdfs:label>date last status update</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#hasHeight -->


    <owl:DatatypeProperty rdf:about="https://auroral.iot.linkeddata.es/def/adapters#hasHeight">
        <rdfs:domain rdf:resource="https://auroral.iot.linkeddata.es/def/adapters#SoilMoisture"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>The height level of the soil moisture</rdfs:comment>
        <rdfs:label>has height</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#hasVolume -->


    <owl:DatatypeProperty rdf:about="https://auroral.iot.linkeddata.es/def/adapters#hasVolume">
        <rdfs:domain rdf:resource="https://auroral.iot.linkeddata.es/def/adapters#SoilMoisture"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>The volume of the soil moisture</rdfs:comment>
        <rdfs:label>has volume</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#title -->


    <owl:DatatypeProperty rdf:about="https://auroral.iot.linkeddata.es/def/adapters#title">
        <rdfs:domain rdf:resource="https://auroral.iot.linkeddata.es/def/adapters#StatusProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The title of the status property</rdfs:comment>
        <rdfs:label>title</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://auroral.iot.linkeddata.es/def/core#hasId -->


    <owl:DatatypeProperty rdf:about="https://auroral.iot.linkeddata.es/def/core#hasId">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The identifier of a given entity</rdfs:comment>
        <rdfs:label>has id</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://auroral.iot.linkeddata.es/def/core#serialNumber -->


    <owl:DatatypeProperty rdf:about="https://auroral.iot.linkeddata.es/def/core#serialNumber">
        <rdfs:domain rdf:resource="https://saref.etsi.org/core#Device"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The serial number of a device</rdfs:comment>
        <rdfs:label>serial number</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://auroral.iot.linkeddata.es/def/core#value -->


    <owl:DatatypeProperty rdf:about="https://auroral.iot.linkeddata.es/def/core#value">
        <rdfs:domain rdf:resource="https://auroral.iot.linkeddata.es/def/core#Readability"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>The readability value of the detected measurement</rdfs:comment>
        <rdfs:label>value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://auroral.iot.linkeddata.es/def/core#versionNumber -->


    <owl:DatatypeProperty rdf:about="https://auroral.iot.linkeddata.es/def/core#versionNumber">
        <rdfs:domain rdf:resource="https://saref.etsi.org/core#Device"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The version number of the employed device</rdfs:comment>
        <rdfs:label>version number</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://saref.etsi.org/core#hasTimeStamp -->


    <owl:DatatypeProperty rdf:about="https://saref.etsi.org/core#hasTimeStamp">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>The timestapm of a given entity</rdfs:comment>
        <rdfs:label>has time stamp</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://saref.etsi.org/core#hasValue -->


    <owl:DatatypeProperty rdf:about="https://saref.etsi.org/core#hasValue">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>The value of the detected measurement</rdfs:comment>
        <rdfs:label>has value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#Accelerometer -->


    <owl:Class rdf:about="https://auroral.iot.linkeddata.es/def/adapters#Accelerometer">
        <rdfs:subClassOf rdf:resource="https://auroral.iot.linkeddata.es/def/core#Sensor"/>
        <rdfs:comment>An instrument for measuring acceleration or for detecting and measuring vibrations</rdfs:comment>
        <rdfs:label>Accelerometer</rdfs:label>
    </owl:Class>
    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#AccumulatedPercipitation -->


    <owl:Class rdf:about="https://auroral.iot.linkeddata.es/def/adapters#AccumulatedPercipitation">
        <rdfs:subClassOf rdf:resource="https://saref.etsi.org/core#Property"/>
        <rdfs:comment>Accumulated precipitation is the sum of precipitation over a certain period of time in a certain place.</rdfs:comment>
        <rdfs:label>Accumulated Percipitation</rdfs:label>
    </owl:Class>
    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#ActReason -->


    <owl:Class rdf:about="https://auroral.iot.linkeddata.es/def/adapters#ActReason">
        <rdfs:subClassOf rdf:resource="https://auroral.iot.linkeddata.es/def/adapters#StatusProperty"/>
        <rdfs:comment>Refers to the act reason of the devices, that change the status of the device from active to passive  or vice versa</rdfs:comment>
        <rdfs:label>Act reason</rdfs:label>
    </owl:Class>
    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#AirConditionningBasic -->


    <owl:Class rdf:about="https://auroral.iot.linkeddata.es/def/adapters#AirConditionningBasic">
        <rdfs:subClassOf rdf:resource="https://auroral.iot.linkeddata.es/def/adapters#CoolingSensor"/>
        <rdfs:subClassOf rdf:resource="https://auroral.iot.linkeddata.es/def/adapters#HeatingSensor"/>
        <rdfs:comment>Air conditioning is the controlling of air temperature, its humidity, its purity, noise- free and its flow rate at a certain place to produce a comfort zone.</rdfs:comment>
        <rdfs:label>Air Conditionning Basic</rdfs:label>
    </owl:Class>
    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#AirQuality -->


    <owl:Class rdf:about="https://auroral.iot.linkeddata.es/def/adapters#AirQuality">
        <rdfs:subClassOf rdf:resource="https://auroral.iot.linkeddata.es/def/adapters#AmbientProperty"/>
        <rdfs:comment>Air quality refers to the degree to which the air is suitable or clean enough for humans or the environment.</rdfs:comment>
        <rdfs:label>Air Quality</rdfs:label>
    </owl:Class>
    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#AirQualitySensor -->


    <owl:Class rdf:about="https://auroral.iot.linkeddata.es/def/adapters#AirQualitySensor">
        <rdfs:subClassOf rdf:resource="https://auroral.iot.linkeddata.es/def/core#Sensor"/>
        <rdfs:comment>An air quality sensor is a device used to detect the concentration of pollutants in the air. It can usually measure two or more air quality indicators such as temperature, humidity, carbon dioxide, carbon monoxide, ozone, formaldehyde, benzene, PM2.5, PM10, etc.</rdfs:comment>
        <rdfs:label>Air Quality Sensor</rdfs:label>
    </owl:Class>
    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#Alert -->


    <owl:Class rdf:about="https://auroral.iot.linkeddata.es/def/adapters#Alert">
        <rdfs:subClassOf rdf:resource="https://auroral.iot.linkeddata.es/def/adapters#StatusProperty"/>
        <rdfs:comment>A mechanism that gives an audible, visual or other kind of alarm signal to alert someone to a problem or condition that requires urgent attention.</rdfs:comment>
        <rdfs:label>Alert</rdfs:label>
    </owl:Class>
    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#AmbientProperty -->


    <owl:Class rdf:about="https://auroral.iot.linkeddata.es/def/adapters#AmbientProperty">
        <rdfs:subClassOf rdf:resource="https://saref.etsi.org/core#Property"/>
        <rdfs:comment>This concepts regroups the ambient properties that are detcetd by the deployed devices. These properties are the ambient temperature, the ambient humidity, the ambient flood, the ambient wind and the ambient solar radiation properties.</rdfs:comment>
        <rdfs:label>Ambient Property</rdfs:label>
    </owl:Class>
    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#AmbientSolarRadiation -->


    <owl:Class rdf:about="https://auroral.iot.linkeddata.es/def/adapters#AmbientSolarRadiation">
        <rdfs:subClassOf rdf:resource="https://auroral.iot.linkeddata.es/def/adapters#AmbientProperty"/>
        <rdfs:comment>Often called the solar resource or just sunlight, is a general term for the electromagnetic radiation emitted by the sun.</rdfs:comment>
        <rdfs:label>Ambient Solar Radiation</rdfs:label>
    </owl:Class>
    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#Ammonia -->


    <owl:Class rdf:about="https://auroral.iot.linkeddata.es/def/adapters#Ammonia">
        <rdfs:subClassOf rdf:resource="https://auroral.iot.linkeddata.es/def/adapters#AirQuality"/>
        <rdfs:comment>Ammonia is an inorganic chemical compound of nitrogen and hydrogen with the formula NH3. A stable binary hydride and the simplest pnictogen hydride, ammonia is a colourless gas with a distinct pungent smell.</rdfs:comment>
        <rdfs:label>Ammonia</rdfs:label>
    </owl:Class>
    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#BoilerBiomass -->


    <owl:Class rdf:about="https://auroral.iot.linkeddata.es/def/adapters#BoilerBiomass">
        <rdfs:subClassOf rdf:resource="https://auroral.iot.linkeddata.es/def/adapters#BoilerSensor"/>
        <rdfs:comment>biomass boilers are boilers that run on biomass fuels.</rdfs:comment>
        <rdfs:label>Boiler Biomass</rdfs:label>
    </owl:Class>
    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#BoilerNgas -->


    <owl:Class rdf:about="https://auroral.iot.linkeddata.es/def/adapters#BoilerNgas">
        <rdfs:subClassOf rdf:resource="https://auroral.iot.linkeddata.es/def/adapters#BoilerSensor"/>
        <rdfs:comment>A sensor boilers that is run on natural gas</rdfs:comment>
        <rdfs:label>Boiler Ngas</rdfs:label>
    </owl:Class>
    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#BoilerOil -->


    <owl:Class rdf:about="https://auroral.iot.linkeddata.es/def/adapters#BoilerOil">
        <rdfs:subClassOf rdf:resource="https://auroral.iot.linkeddata.es/def/adapters#BoilerSensor"/>
        <rdfs:comment>An oil boiler works in a very similar way to a gas boiler. The fuel, in this case oil.</rdfs:comment>
        <rdfs:label>Boiler Oil</rdfs:label>
    </owl:Class>
    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#BoilerSensor -->


    <owl:Class rdf:about="https://auroral.iot.linkeddata.es/def/adapters#BoilerSensor">
        <rdfs:subClassOf rdf:resource="https://auroral.iot.linkeddata.es/def/adapters#HeatingSensor"/>
        <rdfs:comment>A boiler is a device which burns gas, oil, electricity, or coal in order to provide hot water, especially for the central heating in a building</rdfs:comment>
        <rdfs:label>Boiler Sensor</rdfs:label>
    </owl:Class>
    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#CarbonDioxide -->


    <owl:Class rdf:about="https://auroral.iot.linkeddata.es/def/adapters#CarbonDioxide">
        <rdfs:subClassOf rdf:resource="https://auroral.iot.linkeddata.es/def/adapters#AirQuality"/>
        <rdfs:comment>Carbon dioxide is a chemical compound with the chemical formula CO2. It is made up of molecules that each have one carbon atom covalently double bonded to two oxygen atoms.</rdfs:comment>
        <rdfs:label>Carbon Dioxide</rdfs:label>
    </owl:Class>
    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#CarbonMonoxide -->


    <owl:Class rdf:about="https://auroral.iot.linkeddata.es/def/adapters#CarbonMonoxide">
        <rdfs:subClassOf rdf:resource="https://auroral.iot.linkeddata.es/def/adapters#AirQuality"/>
        <rdfs:comment>Carbon monoxide (CO) is a colorless, practically odorless, and tasteless gas or liquid. It results from incomplete oxidation of carbon in combustion. Burns with a violet flame.</rdfs:comment>
        <rdfs:label>Carbon Monoxide</rdfs:label>
    </owl:Class>
    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#CloudCover -->


    <owl:Class rdf:about="https://auroral.iot.linkeddata.es/def/adapters#CloudCover">
        <rdfs:subClassOf rdf:resource="https://auroral.iot.linkeddata.es/def/adapters#AmbientProperty"/>
        <rdfs:comment>Cloud cover (also known as cloudiness, cloudage, or cloud amount) refers to the fraction of the sky obscured by clouds on average when observed from a particular location.</rdfs:comment>
        <rdfs:label>Cloud Cover</rdfs:label>
    </owl:Class>
    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#CoarseParticulateMatter -->


    <owl:Class rdf:about="https://auroral.iot.linkeddata.es/def/adapters#CoarseParticulateMatter">
        <rdfs:subClassOf rdf:resource="https://auroral.iot.linkeddata.es/def/adapters#AirQuality"/>
        <rdfs:comment>Coarse particles (also known as PM10-2.5): particles with diameters generally larger than 2.5 µm and smaller than, or equal to, 10 µm in diameter.</rdfs:comment>
        <rdfs:label>Coarse Particulate Matter</rdfs:label>
    </owl:Class>
    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#CoolingSensor -->


    <owl:Class rdf:about="https://auroral.iot.linkeddata.es/def/adapters#CoolingSensor">
        <rdfs:subClassOf rdf:resource="https://auroral.iot.linkeddata.es/def/core#Sensor"/>
        <rdfs:comment>The Cooling Sensor is used to measure the temperature of the engine coolant.</rdfs:comment>
        <rdfs:label>Cooling Sensor</rdfs:label>
    </owl:Class>
    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#EnergyMonitor -->


    <owl:Class rdf:about="https://auroral.iot.linkeddata.es/def/adapters#EnergyMonitor">
        <rdfs:subClassOf rdf:resource="https://auroral.iot.linkeddata.es/def/core#Sensor"/>
        <rdfs:comment>It is used to track and control real time energy use. It has different properties such as the returned energy, the total returned energy, etc...</rdfs:comment>
        <rdfs:label>Energy Monitor Sensor</rdfs:label>
    </owl:Class>
    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#EnergyProperty -->


    <owl:Class rdf:about="https://auroral.iot.linkeddata.es/def/adapters#EnergyProperty">
        <rdfs:subClassOf rdf:resource="https://saref.etsi.org/core#Property"/>
        <rdfs:comment>It regroups the properties related to the energy such as: the battery level, the totalreturnedEnergy, the TotalEnergy, the PowerFactor, the Gridvoltage, and so one.</rdfs:comment>
        <rdfs:label>Energy Property</rdfs:label>
    </owl:Class>
    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#Error -->


    <owl:Class rdf:about="https://auroral.iot.linkeddata.es/def/adapters#Error">
        <rdfs:subClassOf rdf:resource="https://auroral.iot.linkeddata.es/def/adapters#StatusProperty"/>
        <rdfs:comment>Refers to the rror status of a given device</rdfs:comment>
        <rdfs:label>Error</rdfs:label>
    </owl:Class>
    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#FineParticlesMatter -->


    <owl:Class rdf:about="https://auroral.iot.linkeddata.es/def/adapters#FineParticlesMatter">
        <rdfs:subClassOf rdf:resource="https://auroral.iot.linkeddata.es/def/adapters#AirQuality"/>
        <rdfs:comment>The fine particles, or particulate matter 2.5 (PM2.5), refers to tiny particles or droplets in the air that are 2 ½ microns or less in width.</rdfs:comment>
        <rdfs:label>Fine Particles Matter</rdfs:label>
    </owl:Class>
    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#FloodAmbientProperty -->


    <owl:Class rdf:about="https://auroral.iot.linkeddata.es/def/adapters#FloodAmbientProperty">
        <rdfs:subClassOf rdf:resource="https://auroral.iot.linkeddata.es/def/adapters#AmbientProperty"/>
        <rdfs:comment>a rising and overflowing of a body os water especially onto normally dry land</rdfs:comment>
        <rdfs:label>Flood Ambient Property</rdfs:label>
    </owl:Class>
    


    <!-- https://auroral.iot.linkeddata.es/def/adapters#FloodSensor -->


    <owl:Class rdf:about="https://auroral.iot.linkeddata.es/def/adapters#FloodSensor">
        <rdfs:subClassOf rdf:resource="https://auroral.iot.linkeddata.es/def/core#Sensor"/>
        <rdfs:comment>It is designed for detection of water leaks or detecting excessively high-water level.</rdfs:comment>
        <rdfs:label>Flood Sensor</rdfs:label>
    </owl:Class>
    


